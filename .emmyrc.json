{
	"$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
	"hint": {
		"enable": true,
		"enumParamHint": true,
		"indexHint": true,
		"localHint": true,
		"metaCallHint": true,
		"overrideHint": true,
		"paramHint": true
	},
	"codeLens": {
		"enable": true
	},
	"strict": {
		"arrayIndex": false,
		"docBaseConstMatchBaseType": false,
		"metaOverrideFileDefine": false,
		"requirePath": false,
		"typeCall": false
	},
	"runtime": {
		"classDefaultCall": {
			"forceNonColon": false,
			"forceReturnSelf": false,
			"functionName": ""
		},
		"extensions": [],
		"frameworkVersions": [],
		"nonstandardSymbol": [],
		"requireLikeFunction": [],
		"requirePattern": ["?.lua", "?/init.lua", "lib/?.lua"],
		"special": {},
		"version": "LuaJIT"
	},

	"workspace": {
		"enableReindex": false,
		"encoding": "utf-8",
		"ignoreDir": [],
		"ignoreGlobs": [],
		"library": ["/usr/share/nvim/runtime/", "$HOME/.local/share/nvim/lazy/"],
		"moduleMap": [],
		"preloadFileSize": 0,
		"reindexDuration": 5000,
		"workspaceRoots": []
	}
}
