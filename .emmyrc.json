{
	"$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
	"codeAction": {
		"insertSpace": true
	},
	"hint": {
		"enable": true,
		"enumParamHint": true,
		"indexHint": true,
		"localHint": true,
		"metaCallHint": true,
		"overrideHint": true,
		"paramHint": true
	},
	"diagnostics": {
		"diagnosticInterval": 0,
		"disable": [],
		"enable": true,
		"enables": [],
		"globals": ["*"],
		"globalsRegex": ["*"],
		"severity": {}
	},
	"runtime": {
		"classDefaultCall": {
			"forceNonColon": false,
			"forceReturnSelf": false,
			"functionName": ""
		},
		"extensions": [],
		"frameworkVersions": [],
		"nonstandardSymbol": [],
		"requireLikeFunction": [],
		"requirePattern": [],
		"special": {},
		"version": "LuaJIT"
	},
	"format": {
		"externalTool": null,
		"externalToolRangeFormat": null,
		"useDiff": true
	}
}
